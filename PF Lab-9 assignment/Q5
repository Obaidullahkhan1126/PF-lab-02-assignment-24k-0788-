void reverse(char str[10]){
	int length= strlen(str);
	char rstr[10];
	int y=0;
	for(int i=length-1;i>=0;i--){
		rstr[y]=str[i];
		y++;
		
		
	}
	rstr[y]='\0';
	printf("reversed string: %s",rstr);


	
	
}
#include<stdio.h>
#include<string.h>
int main(){
	char str[10];
	printf("Enter a string: ");
	scanf("%s",str);
	reverse(str);
return 0;
}
