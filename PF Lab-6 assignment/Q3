#include<stdio.h>
int main(){
	int num,flag,i,ans;
	printf("Enter a number");
	scanf("%d",&num);
	flag = 0;
	 i = 2;
	while(i<= num-1 && flag==0){
		ans = num%i;
		if(ans ==0){
		 flag=1;}
		else
		 i++
	}		
	if (flag==1){
	printf("It is composite");}
    else 
     printf("It is prime");
return 0;
} 
